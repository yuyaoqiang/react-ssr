{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "semi": [0],
    "quotes": [1, "double"],
    "react/prefer-stateless-function":[0],
    "import/no-extraneous-dependencies":[0],
    "react/jsx-filename-extension":[0],
    "space-before-blocks":[0],
    "comma-dangle":[0],
    "arrow-parens":[0],
    "global-require":[0]
    }
}
